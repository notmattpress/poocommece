{
	"name": "poocommerce/classic-shortcode",
	"version": "1.0.0",
	"title": "Classic Shortcode",
	"description": "Renders classic PooCommerce shortcodes.",
	"category": "poocommerce",
	"keywords": [
		"PooCommerce"
	],
	"supports": {
		"align": true,
		"html": false,
		"multiple": false,
		"reusable": false,
		"inserter": true
	},
	"attributes": {
		"shortcode": {
			"type": "string",
			"default": "cart",
			"enum": [
				"cart",
				"checkout"
			]
		},
		"align": {
			"type": "string",
			"default": "wide"
		}
	},
	"textdomain": "poocommerce",
	"apiVersion": 3,
	"$schema": "https://schemas.wp.org/trunk/block.json"
}