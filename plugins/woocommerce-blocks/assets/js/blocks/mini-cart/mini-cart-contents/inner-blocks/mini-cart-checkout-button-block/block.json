{
	"name": "poocommerce/mini-cart-checkout-button-block",
	"version": "1.0.0",
	"title": "Mini-Cart Proceed to Checkout Button",
	"description": "Block that displays the checkout button when the Mini-Cart has products.",
	"category": "poocommerce",
	"supports": {
		"align": false,
		"html": false,
		"multiple": false,
		"reusable": false,
		"inserter": true,
		"color": {
			"text": true,
			"background": true
		}
	},
	"attributes": {
		"lock": {
			"type": "object",
			"default": {
				"remove": false,
				"move": false
			}
		}
	},
	"styles": [
		{
			"name": "fill",
			"label": "Fill",
			"isDefault": true
		},
		{
			"name": "outline",
			"label": "Outline"
		}
	],
	"parent": [
		"poocommerce/mini-cart-footer-block"
	],
	"textdomain": "poocommerce",
	"apiVersion": 3,
	"$schema": "https://schemas.wp.org/trunk/block.json"
}