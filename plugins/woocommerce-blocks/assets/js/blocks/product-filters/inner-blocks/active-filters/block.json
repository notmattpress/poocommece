{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "poocommerce/product-filter-active",
	"version": "1.0.0",
	"title": "Active (Experimental)",
	"description": "Display the currently active filters.",
	"category": "poocommerce",
	"keywords": [
		"PooCommerce"
	],
	"textdomain": "poocommerce",
	"apiVersion": 3,
	"ancestor": [
		"poocommerce/product-filters"
	],
	"supports": {
		"interactivity": true,
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": false,
				"radius": false,
				"style": false,
				"width": false
			}
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": false,
			"__experimentalDefaultControls": {
				"margin": false,
				"padding": false,
				"blockGap": false
			}
		}
	},
	"usesContext": [
		"activeFilters"
	],
	"attributes": {
		"clearButton": {
			"type": "boolean",
			"default": false
		}
	}
}
